{
    "name": "choult/tote",
    "description": "A set of simple PSR-11 containers",
    "homepage": "http://choult.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Hoult",
            "email": "chris@choult.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "container-interop/container-interop": "1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "squizlabs/php_codesniffer": "2.3.*",
        "phpmd/phpmd" : "2.2.*",
        "sebastian/phpcpd" : "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "Choult\\Tote\\": "src/",
            "Choult\\Tote\\Test\\": "test/unit"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit; ./vendor/bin/phpcs --standard=psr2 src test; ./vendor/bin/phpmd src,test text ./phpmd.xml.dist,unusedcode,design"
    },
    "provide": {
        "container-interop/container-interop": "1.1.0"
    }
}